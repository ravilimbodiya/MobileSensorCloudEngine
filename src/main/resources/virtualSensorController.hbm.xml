<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.cloud.entity.VirtualSensorController" table="virtual_sensor_controllers">
		<id name="vsControllerId" type="integer">
		    <column name="vs_controller_id" />
			<generator class="identity" />
		</id>
		
		<property name="vsControllerName" column="vs_controller_name" type="string"></property>
		<property name="cpuUtilization" column="cpu_utilization" type="double"></property>
		<property name="memoryAvailable" column="memory_available" type="double"></property>
	</class>
</hibernate-mapping>

